e":72,"column":56}},{"name":"saveFile","generated":{"line":77,"column":4},"source":"js/game.js","original":{"line":72,"column":56}},{"generated":{"line":77,"column":12},"source":"js/game.js","original":{"line":72,"column":64}},{"generated":{"line":77,"column":14},"source":"js/game.js","original":{"line":72,"column":66}},{"generated":{"line":78,"column":0},"source":"js/game.js","original":{"line":72,"column":12}},{"generated":{"line":78,"column":3},"source":"js/game.js","original":{"line":72,"column":2}},{"generated":{"line":79,"column":0},"source":"js/game.js","original":{"line":73,"column":1}},{"name":"window","generated":{"line":81,"column":0},"source":"js/game.js","original":{"line":75,"column":0}},{"generated":{"line":81,"column":6},"source":"js/game.js","original":{"line":75,"column":6}},{"name":"addEventListener","generated":{"line":81,"column":7},"source":"js/game.js","original":{"line":75,"column":7}},{"generated":{"line":81,"column":23},"source":"js/game.js","original":{"line":75,"column":0}},{"generated":{"line":81,"column":24},"source":"js/game.js","original":{"line":75,"column":24}},{"generated":{"line":81,"column":42},"source":"js/game.js","original":{"line":75,"column":0}},{"name":"main","generated":{"line":81,"column":44},"source":"js/game.js","original":{"line":75,"column":44}},{"generated":{"line":81,"column":48},"source":"js/game.js","original":{"line":75,"column":0}}],"sources":{"js/game.js":"import GuessInput from './components/GuessInput';\nimport Baseball from './components/Baseball';\nimport App from './components/App';\nimport { getRandomInt } from './utils';\n\nfunction main() {\n  const params = new URLSearchParams(location.search);\n  const baseball = new Baseball({ el: document.querySelector('.result-container') });\n  const guessInput = new GuessInput({ el: document.querySelector('#guess') });\n\n  baseball\n    .addAction('init', (context, value) => {\n      // { problem: '', results: [] }\n      context.state = value;\n    })\n    .addAction('makeProblem', (context, value) => {\n      context.state.problem = new Array(value).fill(0).reduce((result) => {\n        let randomNumber;\n\n        while (true) {\n          randomNumber = getRandomInt(0, value);\n\n          if (!result.some((r) => r === randomNumber)) {\n            break;\n          }\n        }\n\n        result.push(randomNumber);\n\n        return result;\n      }, []);\n    })\n    .addAction('result', (context, value) => {\n      context.state.guesses.push(value);\n    });\n\n  guessInput\n    .addAction('init', (context, value) => {\n      context.state.digitNumber = value;\n      context.el.addEventListener('keydown', (e) => {\n        if (e.keyCode === 13) {\n          try {\n            const values = Array.from(e.target.value).map((v) => Number(v));\n\n            if (values.length !== context.state.digitNumber) {\n              throw new Error('자릿수가 맞지 않습니다.');\n            }\n\n            context.dispatch('guess', e.target.value).dispatch('clear');\n          } catch (e) {\n            context.dispatch('error', e);\n          }\n        }\n      });\n    })\n    .addAction('clear', (context) => {\n      context.el.value = '';\n    })\n    .addAction('guess', (_, value) => {\n      baseball.dispatch('result', value);\n    })\n    .addAction('error', (_, error) => {\n      alert(error.message);\n    });\n\n  const app = new App({\n    guessInput,\n    baseball,\n  });\n\n  console.log(baseball);\n  app.start({ digitNumber: Number(params.get('digit')), saveFile: null });\n}\n\nwindow.addEventListener('DOMContentLoaded', main);\n"},"lineCount":null}},"error":null,"hash":"0cbc27aeb88122b9f79fb019d093744e","cacheData":{"env":{}}}